 	fc:FocoGrupo(grupo == group)
 	gr:Grupo(grupo == group);
 then
 	TareaSincrona tarea = gestorTareas.crearTareaSincrona(NotificarAlGrupoSinContexto.class);
    tarea.ejecutar(fc);
    gr.actividad();
